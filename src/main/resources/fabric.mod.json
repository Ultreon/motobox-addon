{
  "schemaVersion": 1,
  "id": "motobox",
  "version": "${version}",
  "name": "Motobox",
  "description": "Addon to Automobility",
  "authors": [
    "Qboi",
    "DrAlpha"
  ],
  "contact": {
    "website": "https://ultreon.github.io/web/",
    "repo": "https://github.com/Ultreon/motobox-updated"
  },
  "accessWidener": "motobox.accesswidener",
  "license": "MIT",
  "icon": "assets/motobox/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.ultreon.mods.motobox.client.MotoboxClient"
      }
    ],
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.ultreon.mods.motobox.Motobox"
      }
    ]
  },
  "mixins": [
    "motobox.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.10",
    "fabric": "*",
    "minecraft": "1.19.1",
    "fabric-language-kotlin": ">=1.8.6+kotlin.1.7.21"
  }
}
